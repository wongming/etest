import os
import case3
path = '/Users/wangming/workspace/stest/ts_demo/'
os.chdir(path+'conf')
setup = getattr(__import__('setUp'),'setUp')
teardown = getattr(__import__('tearDown'),'tearDown')
runtest = getattr(__import__('exec'),'runTest')
suite  = case3.Suite(setup=setup,teardown=teardown)
dir(suite)
suite.runTests()
cc = case3.Case()
cc.runTests = runtest
suite.cases.append(cc)
suite.runTests()